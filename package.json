{
  "name": "luna-starter-theme",
  "version": "0.0.0",
  "description": "93Digital's development starter theme, the boilerplate for all our WordPress projects",
  "main": "build/index.js",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:scripts": "wp-scripts build js/index.js js/blocks/blocks.js",
    "build:styles": "sass sass:",
    "build:postcss": "postcss -r style.css && postcss -r style-editor.css",
    "watch": "run-p \"watch:*\"",
    "watch:scripts": "wp-scripts start js/index.js js/blocks/blocks.js",
    "watch:styles": "sass --watch sass:",
    "wp-env": "wp-env"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/93developers/luna-starter-theme.git"
  },
  "keywords": [
    "93digital",
    "starter",
    "theme",
    "boilerplate"
  ],
  "author": "93Devs",
  "license": "ISC",
  "homepage": "https://bitbucket.org/93developers/luna-starter-theme#readme",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@wordpress/api-fetch": "^3.21.5",
    "@wordpress/block-editor": "^5.2.7",
    "@wordpress/blocks": "^7.0.3",
    "@wordpress/browserslist-config": "^2.2.2",
    "@wordpress/components": "^12.0.6",
    "@wordpress/element": "^2.19.1",
    "@wordpress/eslint-plugin": "^8.0.2",
    "@wordpress/html-entities": "^2.10.1",
    "@wordpress/i18n": "^3.18.0",
    "@wordpress/icons": "^2.9.1",
    "@wordpress/scripts": "^13.0.2",
    "@wordpress/url": "^2.21.2",
    "chokidar-cli": "^2.1.0",
    "classnames": "^2.2.6",
    "cssnano": "^4.1.10",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.35",
    "postcss-cli": "^7.1.2",
    "postcss-focus": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "sass": "^1.32.7",
    "stylelint": "^13.10.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-order": "^4.1.0",
    "stylint": "^2.0.0",
    "vanilla-lazyload": "^17.3.1"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
